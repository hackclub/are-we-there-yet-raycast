{
  "name": "are-we-nearly-there-yet",
  "title": "Are We Nearly There Yet?",
  "description": "Show the progress of the Slack migration",
  "icon": "command-icon.png",
  "author": "Leo",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Migration Status",
      "description": "Show the Slack migration progress",
      "mode": "menu-bar",
      "interval": "10m"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.93.0",
    "@raycast/utils": "^1.17.0"
  },
  "devDependencies": {
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5",
    "@raycast/eslint-config": "^1.0.8"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
